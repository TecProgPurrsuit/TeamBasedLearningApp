{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "no-console": ["error", { allow: ["warn", "error"] }],
    "arrow-body-style": 0,
    "react/forbid-prop-types": [1, { "forbid": [<string>] }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/no-static-element-interactions": 0,
    "complexity": ["error", { "max": 4 }],

    }
  }
